{
  "Name": "E7 - Hurricane Wind + Water",
  "Author": "Kuanshi Zhong, Stevan Gavrilovic",
  "units": {
    "force": "kips",
    "length": "ft"
  },
  "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": false
  },
  "RegionalEvent": {
    "units": {
      "PIH": "ft",
      "PWS": "mph"
    },
    "eventFile": "IMs/EventGrid.csv"
  },
  "Applications": {
      "Assets" : {
          "Buildings": {
              "Application": "CSV_to_AIM",
              "ApplicationData": {
                  "filter": "1-20",
                  "assetSourceFile": "AtlanticBuildingInventory.csv"
              }
          }
    },
    "Hazard": {
      "Application": "UserInputHurricane",
      "ApplicationData": {}
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "samples": 10,
        "neighbors": 4
      }
    },
    "Events": [
      {
        "EventClassification": "Hurricane",
        "Application": "SimCenterEvent",
        "ApplicationData": {}
      }
    ],
    "Simulation": {
      "Application": "IMasEDP",
      "ApplicationData": {}
    },
    "DL": {
      "Application": "pelicun",
      "ApplicationData": {
        "DL_Method": "HAZUS MH HU",
        "Realizations": "5000",
        "coupled_EDP": true,
        "detailed_results": false,
        "log_file": true,
        "auto_script": "auto_HU_NJ.py"
      }
    }
  }
}

