{
	"Name": "E11 - Seaside WDN Earthquake",
	"Author": "Stevan Gavrilovic",
	"units": {
		"force": "lb",
		"length": "inch"
	},
	"outputs": {
		"EDP": true,
		"DM": true,
		"DV": true,
		"AIM": true,
		"every_realization": false
	},
	"assets": {
		"buildings": true,
		"water": true
	},
	"RegionalEvent": {
		"units": {
            "PGA": "g",
			"PGV": "cmps"
		},
		"eventFile": "EventGrid.csv"
	},
	"Applications": {
		"Assets" : {
			"Buildings": {
				"Application": "GIS_to_AIM",
				"ApplicationData": {
					"filter": "2000-2010",
					"CRS": "EPSG:4326",
					"assetGISFile": "BuildingInventory/Seaside_Buildings.gpkg"
				}
			},
			"WaterDistributionNetwork": {
				"Application" : "GIS_to_WATERNETWORK",
				"ApplicationData" : {
					"WaterNetworkNodes": {
						"Application": "GIS_to_AIM",
						"ApplicationData": {
							"filter": "1-10",
							"CRS": "EPSG:4326",
							"assetGISFile": "WDN/Nodes/Seaside_water_nodes.shp"
						}
					},
					"WaterNetworkPipelines": {
						"Application": "GIS_to_AIM",
						"ApplicationData": {
							"filter": "1-10",
							"CRS": "EPSG:4326",
							"assetGISFile": "WDN/Pipelines/Seaside_water_pipelines_wgs84.shp"
						}
					}
				}
			}
		},
		"Hazard": {
			"Application": "UserInputRasterHazard",
			"ApplicationData": {
				"rasterFile": "IMs/1000yr/EQ_1000yr.tif",
				"CRS": "EPSG:4326",
				"eventClassification": "Earthquake",
				"bands": ["PGA","PGV"]
				}
		},
		"RegionalMapping": {
			"Application": "SiteSpecifiedEvents"
		},
		"Events": [
			{
				"EventClassification": "Earthquake",
				"Application": "SimCenterEvent",
				"ApplicationData": {}
			}
		],
		"Modeling": {
			"Buildings": {
				"Application": "None",
				"ApplicationData": {}
			},
			"WaterDistributionNetwork": {
				"Application": "None",
				"ApplicationData": {}
			}
		},
		"Simulation": {
			"Buildings": {
				"Application": "IMasEDP",
				"ApplicationData": {}
			},
			"WaterDistributionNetwork": {
				"Application": "IMasEDP",
				"ApplicationData": {}
			}
		},
		"DL": {
			"Buildings": {
				"Application": "Pelicun3",
				"ApplicationData": {
				"DL_Method": "User-provided Fragilities",
				"Realizations": "5",
				"coupled_EDP": true,
				"detailed_results": false,
				"log_file": false,
				"auto_script": "ruleset/custom_pop.py",
				"custom_fragility_dir" : "CustomFragilities"
				}
			},
			"WaterDistributionNetwork": {
				"Application": "CBCitiesDL",
				"ApplicationData": {
				}
			}
		},
		"Performance": {
			"Buildings": {
				"Application": "None",
				"ApplicationData": {
				}
			},
			"WaterDistributionNetwork": {
				"Application": "None",
				"ApplicationData": {
				}
			}
		}
	}
}
