{
  "Name": "E8 - Hurricane Wind",
  "Author": "Kuanshi Zhong, Stevan Gavrilovic",
  "units": {
    "force": "kips",
    "length": "ft"
  },
  "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "BIM": true,
    "every_realization": false
  },
  "RegionalEvent": {
    "units": {
      "PWS": "mph"
    },
    "eventFile": "EventGrid.csv"
  },
  "Applications": {
    "Building": {
      "Application": "GIS_to_BIM",
      "ApplicationData": {
        "filter": "1-20",
        "buildingGISFile": "GISBuildingInventory/BIM_LakeCharles_Full.gpkg"
      }
    },
    "Hazard": {
      "Application": "UserInputRasterHazard",
      "ApplicationData": {
          "rasterFile": "IMs/LakeCharlesWindField.tif",
          "CRS": "EPSG:4326",
          "eventClassification": "Hurricane",
          "bands": ["PWS"]
      }
    },
    "RegionalMapping": {
      "Application": "SiteSpecifiedEvents"
    },
    "Events": [
      {
        "EventClassification": "Hurricane",
        "Application": "SimCenterEvent",
        "ApplicationData": {}
      }
    ],
    "Simulation": {
      "Application": "IMasEDP",
      "ApplicationData": {}
    },
    "DL": {
      "Application": "pelicun",
      "ApplicationData": {
        "DL_Method": "HAZUS MH HU",
        "Realizations": "5000",
        "coupled_EDP": true,
        "detailed_results": false,
        "log_file": true,
        "auto_script": "ruleset/auto_HU_LA.py"
      }
    }
  }
}

