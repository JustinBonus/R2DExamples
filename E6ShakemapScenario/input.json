{
  "Name": "E6 - ShakeMap",
  "Author": "Adam Zsarnoczay, Major Zeng, and Stevan Gavrilovic",
  "units": {
      "force": "kips",
      "length": "ft"
   },
  "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": false
  },
  "RegionalEvent": {
      "Units": {
          "PGA": "g"
      },
      "eventFile": "EventGrid.csv"
  },
  "Applications": {
    "Hazard": {
        "Application": "UserInputShakeMap",
        "ApplicationData": {
	    "Directory": "E6ShakemapScenario/input_data/ShakeMapIMs"
	}
    },	
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "filter": "1-5",
        "buildingSourceFile":"San_Mateo_buildings.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "samples": 100,
        "neighbors": 4
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {}
    }],
    "Simulation": {
       "Application": "IMasEDP",
       "ApplicationData": {}
    },
    "DL": {
       "Application": "pelicun",
       "ApplicationData": {
          "DL_Method": "HAZUS MH EQ IM",
          "Realizations": "100",
          "coupled_EDP": true,
          "event_time": "off",
          "detailed_results": false,
          "log_file": true
       }
    }
  }
}
